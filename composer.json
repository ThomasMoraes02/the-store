{
    "name": "thomas/thestore",
    "description": "thestore ecommerce",
    "require": {
        "slim/slim": "^4.11",
        "mongodb/mongodb": "^1.13",
        "php-di/php-di": "^6.4",
        "slim/psr7": "^1.6",
        "firebase/php-jwt": "^6.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "TheStore\\": "src/"
        },
        "files": [
            "config/config.php","config/container.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "TheStore\\Tests\\": "test/"
        },
        "files": [
            "config/config.php","config/container.php"
        ]
    },
    "authors": [
        {
            "name": "Thomas Moraes",
            "email": "tho.vini7@gmail.com"
        }
    ]
}
